AAAA,8BAAA,IAAA,EAAA,MAAA,CAAA;AAAA,6BAAA,EAAA,KAAA,CAFA;;AAAA;AAEA,4BAAA,GAFA;AAGA,4BAAA,MAAA,CAAA,GAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA;;AACA,4BAAA,MAAA,CAAA,KAAA,CAAA,UAAA,EAAA,6BAAA;;AACA,4BAAA,MAAA,CAAA,UAAA,GAAA,KAAA;AACA,4BAAA,MAAA,CAAA,SAAA,GAAA,KAAA;AACA,4BAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AAPA;AAAA;;AAAA;AAAA;AAAA;AASA,4BAAA,OAAA,CAAA,GAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAAA;AAYA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA,KA/CA;AAgDA,IAAA,GAhDA,eAgDA,IAhDA,EAgDA;AACA,UAAA,IAAA,IAAA,UAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,2BAAA;AACA,OAFA,MAEA,IAAA,IAAA,IAAA,SAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,qBAAA;AACA,OAFA,MAEA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,oBAAA;AACA;AACA;AAxDA;AA1BA,CAAA","sourcesContent":["<template>\n  <card type=\"user\">\n    <p class=\"card-text\"></p>\n    <div class=\"author\">\n      <div class=\"block block-one\"></div>\n      <div class=\"block block-two\"></div>\n      <div class=\"block block-three\"></div>\n      <div class=\"block block-four\"></div>\n      <a href=\"#\">\n        <!-- <img\n          class=\"avatar\"\n          v-if=\"gender == 'M'\"\n          src=\"img/anime3.png\"\n          alt=\"...\"\n        />\n        <img class=\"avatar\" v-else src=\"img/anime6.png\" alt=\"...\" /><br /> -->\n        <span v-if=\"url\"><img class=\"avatar\" :src=\"url\" alt=\"...\"/></span>\n        <span v-else>\n          <img\n            class=\"avatar\"\n            v-if=\"gender == 'M'\"\n            src=\"img/anime3.png\"\n            alt=\"...\"\n          />\n          <img class=\"avatar\" v-else src=\"img/anime6.png\" alt=\"...\" />\n        </span>\n        <br />\n\n        <input\n          v-if=\"edit_photo\"\n          class=\"mx-auto pl-5 text-center text-white\"\n          @change=\"uploadFile\"\n          ref=\"file\"\n          type=\"file\"\n          name=\"image\"\n        />\n        <br v-if=\"edit_photo\" />\n        <span v-if=\"edit_photo\" style=\"color: red;\"\n          >Ensure that your picture is square</span\n        ><br v-if=\"edit_photo\" />\n        <div @click.prevent=\"edit_photo = true\" v-if=\"!edit_photo\" class=\"mt-4\">\n          <vue-loading-button\n            style=\"font-size: 18px;\"\n            class=\"btn btn-primary \"\n            >Upload</vue-loading-button\n          >\n        </div>\n        <div @click.prevent=\"save_picture\" v-if=\"edit_photo\">\n          <vue-loading-button\n            :loading=\"isLoading\"\n            style=\"font-size: 18px;\"\n            class=\"btn btn-primary \"\n            >Upload</vue-loading-button\n          >\n        </div>\n\n        <h3 class=\"title\">{{ name }}</h3>\n      </a>\n\n      <p class=\"description\" v-if=\"!edit_occupation\">\n        {{ occupation }}\n        <i\n          class=\"tim-icons icon-pencil pencil ml-2\"\n          @click=\"edit_occupation = !edit_occupation\"\n        ></i>\n      </p>\n      <base-input\n        v-model=\"change_occupation\"\n        placeholder=\"Occupation\"\n        v-if=\"edit_occupation\"\n      >\n      </base-input>\n      <base-button\n        v-if=\"edit_occupation\"\n        slot=\"footer\"\n        type=\"primary\"\n        fill\n        @click=\"save_occupation\"\n        >Save</base-button\n      >\n      <p class=\"description mt-2\" v-if=\"!edit_status\">\n        <span class=\"mr-2\" v-if=\"!status\">How are you feeling?</span>\n        <span v-else class=\"mr-2\">{{ status }}</span>\n        <i\n          class=\"tim-icons icon-pencil pencil \"\n          @click=\"edit_status = !edit_status\"\n        ></i>\n      </p>\n      <base-input\n        v-model=\"change_status\"\n        placeholder=\"Status\"\n        v-if=\"edit_status\"\n      >\n      </base-input>\n      <base-button\n        v-if=\"edit_status\"\n        slot=\"footer\"\n        type=\"primary\"\n        fill\n        @click=\"save_status\"\n        >Save</base-button\n      >\n    </div>\n    <p></p>\n    <p class=\"card-description\"></p>\n    <div slot=\"footer\" class=\"button-container\">\n      <base-button icon round class=\"btn-facebook\" @click=\"tab('facebook')\">\n        <i class=\"fab fa-facebook\"></i>\n      </base-button>\n      <base-button icon round class=\"btn-twitter\" @click=\"tab('twitter')\">\n        <i class=\"fab fa-twitter\"></i>\n      </base-button>\n      <base-button icon round class=\"btn-google\" @click=\"tab('google')\">\n        <i class=\"fab fa-google-plus\"></i>\n      </base-button>\n    </div>\n  </card>\n</template>\n<script>\nimport API from \"../../api/API\";\nimport BaseButton from \"../../components/BaseButton.vue\";\nimport { CldImage } from \"cloudinary-vue\";\nexport default {\n  components: {\n    BaseButton,\n    CldImage\n  },\n  props: {\n    name: null,\n    occupation: null,\n    gender: null,\n    status: null,\n    email: null,\n    url: null\n  },\n  data() {\n    return {\n      edit_occupation: false,\n      edit_status: false,\n      change_occupation: null,\n      change_status: null,\n      edit_photo: false,\n      images: null,\n      image_name: null,\n      image: null,\n      isLoading: false\n    };\n  },\n  methods: {\n    uploadFile() {\n      this.images = this.$refs.file.files[0];\n      console.log(this.images);\n      this.image_name = this.images.name;\n    },\n    async save_occupation() {\n      this.edit_occupation = !this.edit_occupation;\n      const token = window.localStorage.getItem(\"token\");\n      var occupation = this.change_occupation;\n      try {\n        const res = await API.occupation({ occupation }, token);\n        this.occupation = occupation;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    async save_status() {\n      this.edit_status = !this.edit_status;\n      const token = window.localStorage.getItem(\"token\");\n      var status = this.change_status;\n      try {\n        const res = await API.status({ status }, token);\n        this.status = status;\n      } catch (err) {\n        console.log(err);\n      }\n    },\n    async save_picture() {\n      this.isLoading = true\n      const token = window.localStorage.getItem(\"token\");\n      if (this.images) {\n        const reader = new FileReader();\n        reader.readAsDataURL(this.images);\n        reader.onloadend = async () => {\n          try {\n            const res = await API.uploadPic({ data: reader.result }, token);\n            this.url = res.data.url;\n            this.$emit(\"uploaded\", \"Image successfully uploaded\");\n            this.edit_photo = false;\n            this.isLoading = false;\n            window.location.reload()\n          } catch (err) {\n            console.log(err);\n          }\n        };\n      }\n    },\n    tab(name){\n      if (name == \"facebook\"){\n        window.open(\"https://www.facebook.com/\")\n      } else if (name == \"twitter\"){\n        window.open(\"https://twitter.com\")\n      } else {\n        window.open(\"https://google.com\")\n      }\n    }\n  }\n};\n</script>\n<style>\n.pencil {\n  cursor: pointer;\n}\n</style>\n"],"sourceRoot":"src/pages/Profile"}]}