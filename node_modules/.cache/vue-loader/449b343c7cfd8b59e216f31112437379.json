wMenu, 'navbar-transparent': !showMenu }\"\n  >\n    <div class=\"container-fluid\">\n      <div class=\"navbar-wrapper\">\n        <div\n          class=\"navbar-toggle d-inline\"\n          :class=\"{ toggled: $sidebar.showSidebar }\"\n        >\n          <button\n            type=\"button\"\n            class=\"navbar-toggler\"\n            aria-label=\"Navbar toggle button\"\n            @click=\"toggleSidebar\"\n          >\n            <span class=\"navbar-toggler-bar bar1\"></span>\n            <span class=\"navbar-toggler-bar bar2\"></span>\n            <span class=\"navbar-toggler-bar bar3\"></span>\n          </button>\n        </div>\n        <a class=\"navbar-brand\" href=\"#pablo\">{{ routeName }}</a>\n      </div>\n      <button\n        class=\"navbar-toggler\"\n        type=\"button\"\n        @click=\"toggleMenu\"\n        data-toggle=\"collapse\"\n        data-target=\"#navigation\"\n        aria-controls=\"navigation-index\"\n        aria-label=\"Toggle navigation\"\n      >\n        <span class=\"navbar-toggler-bar navbar-kebab\"></span>\n        <span class=\"navbar-toggler-bar navbar-kebab\"></span>\n        <span class=\"navbar-toggler-bar navbar-kebab\"></span>\n      </button>\n\n      <collapse-transition>\n        <div class=\"collapse navbar-collapse show\" v-show=\"showMenu\">\n          <ul class=\"navbar-nav\" :class=\"$rtl.isRTL ? 'mr-auto' : 'ml-auto'\">\n            <div\n              class=\"search-bar input-group\"\n              @click=\"searchModalVisible = true\"\n            >\n              <!-- <input type=\"text\" class=\"form-control\" placeholder=\"Search...\">\n              <div class=\"input-group-addon\"><i class=\"tim-icons icon-zoom-split\"></i></div> -->\n              <!-- <button class=\"btn btn-link\" id=\"search-button\" data-toggle=\"modal\" data-target=\"#searchModal\">\n                <i class=\"tim-icons icon-zoom-split\"></i>\n              </button> -->\n              <div\n                style=\"align-items: center; margin: auto; font-size: 20px; color: white;\"\n              >\n                Welcome, {{ firstName }}\n              </div>\n              <!-- You can choose types of search input -->\n            </div>\n            <!-- <modal :show.sync=\"searchModalVisible\"\n                   class=\"modal-search\"\n                   id=\"searchModal\"\n                   :centered=\"false\"\n                   :show-close=\"true\">\n              <input slot=\"header\" v-model=\"searchQuery\" type=\"text\" class=\"form-control\" id=\"inlineFormInputGroup\" placeholder=\"SEARCH\">\n            </modal> -->\n            <base-dropdown\n              tag=\"li\"\n              :menu-on-right=\"!$rtl.isRTL\"\n              title-tag=\"a\"\n              class=\"nav-item\"\n            >\n              <a\n                slot=\"title\"\n                href=\"#\"\n                class=\"dropdown-toggle nav-link mt-1\"\n                data-toggle=\"dropdown\"\n                aria-expanded=\"true\"\n              >\n                <div class=\"notification d-none d-lg-block d-xl-block\"></div>\n                <i class=\"tim-icons icon-bell-55\"></i>\n                <p class=\"d-lg-none\">\n                  New Notifications\n                </p>\n              </a>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\"\n                  >The water bill for the month is ready</a\n                >\n              </li>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\"\n                  >You have 1 unpaid bill</a\n                >\n              </li>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\"\n                  >The total consumption is 30% lesser compared to previous\n                  month. Good Job!</a\n                >\n              </li>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\"\n                  >Do you know that you can save more electricity if you use the\n                  fan?</a\n                >\n              </li>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\"\n                  >Remember to switch off unused appliances!</a\n                >\n              </li>\n            </base-dropdown>\n            <base-dropdown\n              tag=\"li\"\n              :menu-on-right=\"!$rtl.isRTL\"\n              title-tag=\"a\"\n              class=\"nav-item\"\n              menu-classes=\"dropdown-navbar\"\n            >\n              <a\n                slot=\"title\"\n                href=\"#\"\n                class=\"dropdown-toggle nav-link\"\n                data-toggle=\"dropdown\"\n                aria-expanded=\"true\"\n              >\n                <div class=\"photo\">\n                  <span v-if=\"url\"\n                    ><img class=\"avatar\" :src=\"url\" alt=\"...\"\n                  /></span>\n                  <span v-else>\n                    <img\n                      class=\"avatar\"\n                      v-if=\"gender == 'M'\"\n                      src=\"img/anime3.png\"\n                      alt=\"...\"\n                    />\n                    <img class=\"avatar\" v-else src=\"img/anime6.png\" alt=\"...\" />\n                  </span>\n                </div>\n                <b class=\"caret d-none d-lg-block d-xl-block\"></b>\n                <p class=\"d-lg-none\">\n                  Log out\n                </p>\n              </a>\n              <li class=\"nav-link\">\n                <router-link to=\"/home/profile\" class=\"nav-item dropdown-item\"\n                  >Profile</router-link\n                >\n              </li>\n              <div class=\"dropdown-divider\"></div>\n              <li class=\"nav-link\">\n                <a href=\"#\" class=\"nav-item dropdown-item\" @click=\"logout\"\n                  >Log out</a\n                >\n              </li>\n            </base-dropdown>\n          </ul>\n        </div>\n      </collapse-transition>\n    </div>\n  </nav>\n</template>\n<script>\nimport { CollapseTransition } from \"vue2-transitions\";\nimport Modal from \"@/components/Modal\";\n\nexport default {\n  components: {\n    CollapseTransition,\n    Modal\n  },\n  props: {\n    gender: null,\n    firstName: null,\n    url: null\n  },\n  computed: {\n    routeName() {\n      const { name } = this.$route;\n      return this.capitalizeFirstLetter(name);\n    },\n    isRTL() {\n      return this.$rtl.isRTL;\n    }\n  },\n  data() {\n    return {\n      activeNotifications: false,\n      showMenu: false,\n      searchModalVisible: false,\n      searchQuery: \"\"\n    };\n  },\n  methods: {\n    capitalizeFirstLetter(string) {\n      return string.charAt(0).toUpperCase() + string.slice(1);\n    },\n    toggleNotificationDropDown() {\n      this.activeNotifications = !this.activeNotifications;\n    },\n    closeDropDown() {\n      this.activeNotifications = false;\n    },\n    toggleSidebar() {\n      this.$sidebar.displaySidebar(!this.$sidebar.showSidebar);\n    },\n    hideSidebar() {\n      this.$sidebar.displaySidebar(false);\n    },\n    toggleMenu() {\n      this.showMenu = !this.showMenu;\n    },\n    redirect() {\n      this.$router.push(\"/home/profile\");\n    },\n    logout() {\n      window.localStorage.clear();\n      this.$router.push(\"/\");\n    }\n  }\n};\n</script>\n<style></style>\n"]}]}